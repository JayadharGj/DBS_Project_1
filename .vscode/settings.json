{
    "cmake.buildDirectory": "${sourceDirectory}/build.${buildType}",
    "cmake.parallelJobs": 8,
    "C_Cpp.intelliSenseCacheSize": 1024,
    "C_Cpp.intelliSenseMemoryLimit": 4096,
    "C_Cpp.intelliSenseCachePath": "/local/Spring_2023/${env:USER}/.cache/vscode-cpptools",
    
    "cmake.configureOnOpen": false,
    "files.associations": {
        "*.inc": "cpp",
        "*.template": "plaintext",
        "**/src/catalog/systables/*.py.inc": "python",
        "**/src/catalog/systables/*.inc": "plaintext",
        "*.in": "cpp",
        "*.dat": "plaintext",
        "atomic": "cpp",
        "*.tcc": "cpp",
        "fstream": "cpp",
        "sstream": "cpp"
    }
}
